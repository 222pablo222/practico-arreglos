#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n;
    cout << "Ingrese la cantidad de numeros de Fibonacci a generar: ";
    cin >> n;

    vector<long long> fib(n); 

    if (n >= 1) fib[0] = 0;  
    if (n >= 2) fib[1] = 1;   

    for (int i = 2; i < n; i++) {
        fib[i] = fib[i-1] + fib[i-2];
    }

    cout << "Los primeros " << n << " numeros de Fibonacci son:\n";
    for (int i = 0; i < n; i++) {
        cout << fib[i] << " ";
    }
    cout << endl;

    return 0;
}
