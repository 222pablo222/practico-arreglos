#include <iostream>
#include <vector>
using namespace std;
int main (){
    int n;
    cout <<"Ingresa la cantidad de numeros : "<<endl;
    cin >> n;
    vector<int> original(n);
    for (int i=0; i<n; i++){
        cout << "ingrese el elemento["<<i+1<<"]: ";
        cin >> original[i];
    }
    vector <int> invertido(n);
    for (int i=0; i<n; i++){
        invertido[i] = original [n - 1 - i];
    }
    
    cout<<"\nEl valor original:\n";
    for (int i=0; i<n; i++){
        cout<<original[i]<<endl;
    }
    cout<<"\n\nLos valores invertidos:"<<endl;
    for (int i=0; i<n; i++){
        cout<<invertido[i]<<endl;
    }
    return 0;
}
