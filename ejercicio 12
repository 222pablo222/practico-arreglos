#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n;
    cout << "Ingrese la cantidad de elementos del vector: ";
    cin >> n;

    if (n <= 0) {
        cout << "El vector debe tener al menos un elemento." << endl;
        return 0;
    }

    vector<int> v(n);
    cout << "Ingrese los elementos del vector:\n";
    for (int i = 0; i < n; i++) {
        cout << "Elemento[" << i + 1 << "]: ";
        cin >> v[i];
    }

    vector<int> sinDuplicados;

    for (int i = 0; i < n; i++) {
        bool existe = false;
        for (int j = 0; j < sinDuplicados.size(); j++) {
            if (v[i] == sinDuplicados[j]) {
                existe = true;
                break;
            }
        }
        if (!existe) {
            sinDuplicados.push_back(v[i]);
        }
    }

    cout << "\nVector sin duplicados:\n";
    for (int i = 0; i < sinDuplicados.size(); i++) {
        cout << sinDuplicados[i] << " ";
    }
    cout << endl;

    return 0;
}

