#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n, m;
    cout << "Ingrese la cantidad de elementos del primer vector: ";
    cin >> n;
    cout << "Ingrese la cantidad de elementos del segundo vector: ";
    cin >> m;

    vector<int> A(n), B(m);
    cout << "Ingrese los elementos del primer vector:\n";
    for (int i = 0; i < n; i++) {
        cout << "A[" << i + 1 << "]: ";
        cin >> A[i];
    }

    cout << "Ingrese los elementos del segundo vector:\n";
    for (int i = 0; i < m; i++) {
        cout << "B[" << i + 1 << "]: ";
        cin >> B[i];
    }
  
    vector<int> C;
    for (int i = 0; i < n; i++) C.push_back(A[i]);
    for (int i = 0; i < m; i++) C.push_back(B[i]);

    cout << "\nVector concatenado: ";
    for (int i = 0; i < C.size(); i++) cout << C[i] << " ";
    cout << endl;

    return 0;
}
