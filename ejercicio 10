#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n, m;
    cout << "Ingrese la cantidad de elementos del primer vector: ";
    cin >> n;
    cout << "Ingrese la cantidad de elementos del segundo vector: ";
    cin >> m;

    vector<int> A(n), B(m);
    cout << "Ingrese los elementos del primer vector (ascendente):\n";
    for (int i = 0; i < n; i++) cin >> A[i];

    cout << "Ingrese los elementos del segundo vector (ascendente):\n";
    for (int i = 0; i < m; i++) cin >> B[i];

    // Vector resultante
    vector<int> C;
    int i = 0, j = 0;

    // Mezclar mientras haya elementos en ambos vectores
    while (i < n && j < m) {
        if (A[i] <= B[j]) {
            C.push_back(A[i]);
            i++;
        } else {
            C.push_back(B[j]);
            j++;
        }
    }

    // Agregar los elementos restantes de A
    while (i < n) {
        C.push_back(A[i]);
        i++;
    }

    // Agregar los elementos restantes de B
    while (j < m) {
        C.push_back(B[j]);
        j++;
    }

    cout << "\nVector intercalado y ordenado:\n";
    for (int k = 0; k < C.size(); k++) cout << C[k] << " ";
    cout << endl;

    return 0;
}
